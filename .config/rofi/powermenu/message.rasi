/* Message Dialog */

@import "black_powermenu.rasi"

* {
    background-color:       		@background;
    text-color:             		@selected;
	font:			 	"Museo Sans 12";
}

window {
    width:      			720px;
    padding:    			12px;
    border:				1px;
    border-radius:          		12px;
    border-color:           		@border;
    location:               		center;
    y-offset:               		-2em;
}

entry {
    expand: 				true;
    text-color:				@border;
}
